{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@layouts": ["./src/@layouts"],
      "@layouts/*": ["./src/@layouts/*"],
      "@pages": ["./src/pages"],
      "@pages/*": ["./src/pages/*"],
      "@types": ["./src/@type"],
      "@types/*": ["./src/@type/*"],
      "@styles": ["./src/@styles"],
      "@styles/*": ["./src/@styles/*"],
      "@hooks": ["./src/@hooks"],
      "@hooks/*": ["./src/@hooks/*"],
      "@components": ["./src/@components"],
      "@components/*": ["./src/@components/*"],
      "@context": ["./src/@context"],
      "@context/*": ["./src/@context/*"],
      "@utils": ["./src/@utils"],
      "@utils/*": ["./src/@utils/*"]
    },
    "types": ["@emotion/react/types/css-prop"],
    "jsxImportSource": "@emotion/react"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
